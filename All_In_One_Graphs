data(cars)
data("mtcars")
View(mtcars)
plot(mtcars$mpg,mtcars$disp)
plot(mtcars$mpg,mtcars$disp,type="p")
round(max(mtcars$mpg))
plot(mtcars$mpg,mtcars$disp,type="p",xlim=round(max(mtcars$mpg)),ylim = max(mtcars$cyl))
max_mpq<-round(max(mtcars$mpg))
